import random
randones = random.randint(1, 10)
if randones <= 5:
 print("El numero es igual o menor que 5")
else:
 print("el numero es mayor que 5")


pregunta = int(input("Dime un numero\n"))
if not randones == pregunta:
        print("NO has acertado")
if randones == pregunta:
    print("has acertado")



while not randones == pregunta:
 pregunta = int(input("Dime un numero\n"))
 if randones == pregunta:
    print("has acertado")
 if not randones == pregunta:
        print("NO has acertado")
