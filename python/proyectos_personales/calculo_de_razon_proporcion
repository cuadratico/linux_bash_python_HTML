print("Razon y proporcion\n")
intentos = 0
while intentos < 100:
  intentos += 1
  try:
   numero1 = int(input("Dime un numero, parte superior izquierad\n"))
   numero2 = int(input("Dime un numero, parte inferior izquierda\n"))
   numero3 = int(input("Dime un numero, parte superior derecha\n"))
   numero4 = int(input("Dime un numero, parte inferior derecha\n"))

   division1 = numero1 / numero2
   division2 = numero3 / numero4

   if division1 == division2:
    print("La razon forma una proporcion")
   else:
    print("La razon no forma una proporcion")
  except:
   ValueError: print("Has escrito un valor que no es entero") #type: ignore
